{
  "name": "scroll-rise",
  "version": "1.5.0",
  "private": false,
  "description": "The typescript library for scroll animation",
  "main": "lib/index.umd.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "test": "karma start --browsers Chrome",
    "test:no-browser": "karma start --browsers ChromeHeadless --single-run",
    "lint": "npx eslint .",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "watch": "webpack --watch --mode production",
    "serve:docs": "webpack serve --config webpack.docs.config.js --mode development",
    "build:docs:dev": "webpack --config webpack.docs.config.js --mode development",
    "build:docs": "webpack --config webpack.docs.config.js --mode production",
    "generate:examples-from-specs": "npx ts-node-esm -P src/docs/spec-examples-generator/tsconfig.spec.examples.json src/docs/spec-examples-generator/generate-spec-examples.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drKaramazin/scroll-rise.git"
  },
  "keywords": [
    "animation",
    "scroll",
    "typescript"
  ],
  "author": "Nikolay Grishkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drKaramazin/scroll-rise/issues"
  },
  "homepage": "https://github.com/drKaramazin/scroll-rise#readme",
  "devDependencies": {
    "@types/jasmine": "~4.0.2",
    "@typescript-eslint/eslint-plugin": "~5.41.0",
    "copy-webpack-plugin": "~11.0.0",
    "eslint": "~8.26.0",
    "eslint-config-standard-with-typescript": "~23.0.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-n": "~15.3.0",
    "eslint-plugin-promise": "~6.1.1",
    "glob": "^8.0.3",
    "html-webpack-plugin": "~5.5.0",
    "jasmine-core": "~4.0.1",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.1",
    "karma-jasmine": "~4.0.2",
    "karma-typescript": "~5.5.3",
    "karma-typescript-es6-transform": "~5.5.3",
    "mustache": "^4.2.0",
    "mustache-loader": "^1.4.3",
    "puppeteer": "~19.2.0",
    "sanitize-filename-truncate": "^1.0.2",
    "ts-loader": "~9.2.7",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4",
    "webpack": "~5.70.0",
    "webpack-cli": "~4.9.2",
    "webpack-dev-server": "~4.7.4"
  }
}
